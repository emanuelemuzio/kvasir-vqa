{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GradCAM fine tune with turn off time",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--turnoff=0",
                "--num_epochs=400",
                "--batch_size=16",
                "--lr=0.01",
                "--momentum=0.9",
                "--T_max=100",
                "--eta_min=0.001",
                "--patience=5",
                "--min_delta=0.01",
                "--scheduler=plateau",
                "--optimizer=adam",
                "--mode=min"
            ]
        },
        {
            "name": "GradCAM fine tune no turn off",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--num_epochs=200",
                "--batch_size=24",
                "--lr=0.0001",
                "--momentum=0.9",
                "--T_max=100",
                "--eta_min=0.001",
                "--patience=5",
                "--min_delta=0.05",
                "--scheduler=plateau",
                "--optimizer=adam",
                "--mode=min"
            ]
        }
    ]
}