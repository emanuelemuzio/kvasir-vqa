{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Fine tune feature extractor",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--turnoff=-1",
                "--num_epochs=300",
