{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GradCAM fine tune with turn off time",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--turnoff=120",
                "--num_epochs=300",
                "--batch_size=25",
                "--lr=0.00001",
                "--momentum=0.9",
                "--T_max=100",
                "--eta_min=0.001",
                "--patience=5",
                "--min_delta=0.025",
                "--scheduler=plateau",
                "--optimizer=sgd",
                "--mode=min"
            ]
        },
        {
            "name": "GradCAM fine tune no turn off",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--num_epochs=300",
                "--batch_size=32",
                "--lr=0.00001",
                "--momentum=0.9",
                "--T_max=100",
                "--eta_min=0.001",
                "--patience=5",
                "--min_delta=0.025",
                "--scheduler=plateau",
                "--optimizer=sgd",
                "--mode=min"
            ]
        }
    ]
}